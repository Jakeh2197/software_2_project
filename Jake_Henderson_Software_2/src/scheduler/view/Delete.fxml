<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="150.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scheduler.view.controller.DeleteController">
   <children>
      <Label layoutX="24.0" layoutY="29.0" text="Are you sure you want to delete this item?">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="YesButton" layoutX="50.0" layoutY="90.0" mnemonicParsing="false" onAction="#YesButtonHandler" prefHeight="30.0" prefWidth="100.0" text="YES" AnchorPane.leftAnchor="50.0">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="NoButton" layoutX="250.0" layoutY="90.0" mnemonicParsing="false" onAction="#NoButtonHandler" prefHeight="30.0" prefWidth="100.0" text="NO" AnchorPane.rightAnchor="50.0">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
